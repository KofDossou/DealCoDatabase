SQL> UPDATE PRODMASTER, PRODSALES
  2  SET PRODMASTER.PROD_QOH = PROD_QOH - PS_QTY
  3  WHERE PRODUCT.PROD_ID = PRODSALES.PROD_ID;
UPDATE PRODMASTER, PRODSALES
